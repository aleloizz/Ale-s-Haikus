{
  "clean_urls": false,
  "https_only": true,
  "headers": {
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable",
      "Vary": "Accept-Encoding"
    },
    "/static/favicon.ico": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.css": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/static/*.js": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/static/css/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/static/js/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/static/images/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.ico": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.svg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.webp": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.jpg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/*.woff2": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/sitemap.xml": {
      "Cache-Control": "public, max-age=3600"
    },
    "/static/robots.txt": {
      "Cache-Control": "public, max-age=86400"
    }
  }
}
