{
  "root": "static/",
  "clean_urls": false,
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=31536000, immutable",
      "Vary": "Accept-Encoding"
    },
    "/favicon.ico": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.css": {
      "Cache-Control": "public, max-age=31536000",
      "Content-Encoding": "gzip"
    },
    "/*.js": {
      "Cache-Control": "public, max-age=31536000", 
      "Content-Encoding": "gzip"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.woff2": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/sitemap.xml": {
      "Cache-Control": "public, max-age=3600"
    },
    "/robots.txt": {
      "Cache-Control": "public, max-age=86400"
    }
  }
}
